    "google-spreadsheet": "^2.0.5",
    "googleapis": "^27.0.0",
    "jethro": "^2.6.6",
    "lodash": "^4.16.3",
    "moment": "^2.15.1",
    "node-schedule": "^1.3.0",
    "request": "^2.87.0",
    "superagent": "^3.5.2",
    "underscore": "^1.8.3"
  },
  "engines": {
    "node": "4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/franciscog/DerpyBot"
  },
